{
  "name": "zero-feeds",
  "version": "1.4.3",
  "engines": [
    "node = 0.8.x"
  ],
  "description": "Read your RSS feeds within your Cozy.",
  "repository": {
    "type": "git",
    "url": "https://github.com/pierrerousseau/zero-feeds.git"
  },
  "main": "server.coffee",
  "dependencies": {
    "americano": "0.3.3",
    "americano-cozy": "0.2.2",
    "iconv": "2.1.6"
  },
  "devDependencies": {
    "should": "= 1.2.0",
    "mocha": "= 1.6.0",
    "chai": "= 1.3.0"
  },
  "scripts": {
    "test": "cake tests",
    "start": "node server.js"
  },
  "cozy-displayName": "zero-feeds",
  "cozy-permissions": {
    "feed": {
      "description": "Allow you to manage your feeds"
    },
    "zfparam": {
      "description": "Allow you to change the options of the zero feed app"
    }
  }
}
