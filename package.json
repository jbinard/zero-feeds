{
  "name": "cozy-feeds",
  "version": "1.3.0",
  "engines": [
    "node = 0.8.x"
  ],
  "description": "Read your RSS feeds within your Cozy.",
  "repository": {
    "type": "git",
    "url": "https://github.com/Piour/cozy-feeds.git"
  },
  "main": "server.coffee",
  "dependencies": {
    "americano": "0.2.13",
    "americano-cozy": "0.1.10"
  },
  "devDependencies": {
    "should": "= 1.2.0",
    "mocha": "= 1.6.0",
    "chai": "= 1.3.0"
  },
  "scripts": {
    "test": "cake tests",
    "start": "node server.js"
  },
  "cozy-displayName": "Feeds",
  "cozy-permissions": {
    "feed": {
      "description": "Allow you to manage your feeds"
    },
    "param": {
      "description": "Allow you to change the options of the app"
    }
  }
}
